{
  "name": "kangaroos-server",
  "version": "0.1.0",
  "description": "The server implementation for the Kangaroos CMS.",
  "main": "converter.js",
  "bin": {
    "kangaroos": "bin/cli.js",
    "kangaroos-server": "bin/cli.js"
  },
  "scripts": {
    "lint": "scripts/lint",
    "test": "scripts/test",
    "watch": "scripts/watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kangaroosio/kangaroos-server"
  },
  "keywords": [
    "kangaroo",
    "cms",
    "content",
    "management",
    "system"
  ],
  "author": "Alexander Martin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kangaroosio/kangaroos-server/issues"
  },
  "homepage": "https://kangaroos.io",
  "dependencies": {
    "async": "^1.5.2",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.14.2",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "^1.4.1",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "jade": "^1.11.0",
    "kerberos": "0.0.18",
    "mongodb": "^2.1.4",
    "node-uuid": "^1.4.7",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport.socketio": "^3.6.1",
    "querystringify": "0.0.3",
    "request": "^2.67.0",
    "semver": "^5.1.0",
    "socket.io": "^1.4.5",
    "superagent": "^1.7.1",
    "url-parse": "^1.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "file-loader": "^0.8.5",
    "jest-cli": "^0.8.2",
    "json-loader": "^0.5.4",
    "nodemon": "^1.8.1",
    "react": "^0.14.6",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.6",
    "react-map-styles": "^0.3.0",
    "reactcss": "^0.4.2",
    "socket.io-client": "^1.4.4",
    "tap": "^5.1.1",
    "uglify-loader": "^1.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12"
  },
  "jest": {
    "testDirectoryName": "frontend/test/unit",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "verbose": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/frontend"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "es6",
      "js",
      "json",
      "jsx"
    ]
  }
}
